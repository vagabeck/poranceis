<script>
  $(document).ready(function(){
      $('.mapa-magico').craftmap({
          fullscreen: true,
          image: {
            width: <%= current_page.data.width %>,
            height: <%= current_page.data.height %>,
          },
          map: {
            position: 'center'
          },
          marker: {
            onClick: function(marker) {
              page = marker[0].id.replace(/[0-9]/g, '');
              document.location.href = page + '.html';
            }
          }
      });
  });
</script>
